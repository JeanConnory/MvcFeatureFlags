{
  "ConnectionStrings": {
    "sqlite": "Data Source=Catalog.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "CreateProduct": false,
    "EditProduct": true,
    "DeleteProduct": false,
    "ListProduct": true,
    "MiddlewareTeste": true,
    "ExportProduct": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "ProductSaleOff": {
      "EnabledFor": [
        {
          "Name": "Microsoft.TimeWindow",
          "Parameters": {
            "Start": "2024-03-28T19:34:00Z",
            "End": "2024-03-28T19:34:59Z"
          }
        }
      ]
    }
  }
}
